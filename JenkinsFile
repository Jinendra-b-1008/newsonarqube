pipeline {
    agent any

     

    stages {
        

        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
        stage("SonarQube analysis"){ 
		    environment{
			    scannerHome = tool 'Sonar-Scanner'
		    }
		    steps {
			withSonarQubeEnv('sonarqube-server'){
				sh "${scannerHome}/bin/sonar-scanner"
			  } 
			}
	    }
    }
}
